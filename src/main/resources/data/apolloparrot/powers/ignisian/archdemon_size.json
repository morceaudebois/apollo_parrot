{
	"type":"origins:multiple",
	"scale":{
		"type":"origins:action_over_time",
		"entity_action":{
			"type":"origins:execute_command",
			"command":"function apolloparrot:ignisian/size"
		},
		"interval":20
	},
	"reset":{
		"type":"origins:action_on_callback",
		"entity_action_lost":{
			"type":"origins:execute_command",
			"command":"scale reset"
		}
	},
	"respawn":{
		"type":"origins:action_on_callback",
		"entity_action_respawned":{
			"type":"origins:execute_command",
			"command":"resource set @s apolloparrot:ignisian/ignisian_zenith 0"
		}
	}
}