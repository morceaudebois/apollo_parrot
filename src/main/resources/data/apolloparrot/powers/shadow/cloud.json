{
	"type":"origins:multiple",
	"resource":{
		"type":"origins:resource",
		"min":0,
		"max":300,
		"start_value":300,
		"hud_render":{
			"sprite_location":"origins:textures/gui/community/spiderkolo/resource_bar_01.png",
			"bar_index":16
		}
	},
	"toggle":{
		"type":"origins:toggle",
		"key":"key.origins.primary_active",
		"active_by_default":false
	},
	"disable_in_sunlight":{
		"type":"origins:toggle",
		"power":"*:*_toggle",
		"condition":{
			"type":"origins:and",
			"conditions":[
				{
					"type":"origins:power_active",
					"power":"*:*_toggle"
				},
				{
					"type":"origins:exposed_to_sun"
				}
			]
		}
	},
	"timer":{
		"type":"origins:action_over_time",
		"interval":1,
		"entity_action":{
			"type":"origins:if_else",
			"condition":{
				"type":"origins:power_active",
				"power":"*:*_toggle"
			},
			"if_action":{
				"type":"origins:change_resource",
				"resource":"*:*_resource",
				"change":-1
			},
			"else_action":{
				"type":"origins:change_resource",
				"resource":"*:*_resource",
				"change":1,
				"condition":{
					"type":"origins:exposed_to_sun",
					"inverted":true
				}
			}
		}
	},
	"invisibility":{
		"type":"origins:invisibility",
		"render_armor":false,
		"condition":{
			"type":"origins:power_active",
			"power":"*:*_toggle"
		}
	},
	"invulnerability":{
		"type":"origins:invulnerability",
		"damage_condition":{
			"type":"origins:or",
			"conditions":[
				{
					"type":"origins:name",
					"name":"inWall"
				},
				{
					"type":"origins:name",
					"name":"fall"
				}
			]
		},
		"condition":{
			"type":"origins:power_active",
			"power":"*:*_toggle"
		}
	},
	"weak":{
		"type":"origins:stacking_status_effect",
		"condition":{
			"type":"origins:power_active",
			"power":"*:*_toggle"
		},
		"min_stacks":0,
		"max_stacks":1,
		"duration_per_stack":20,
		"effect":{
			"show_particles":false,
			"show_icon":false,
			"effect":"minecraft:weakness",
			"amplifier":100
		}
	},
	"particles":{
		"type":"origins:action_over_time",
		"interval":5,
		"entity_action":{
			"type":"origins:execute_command",
			"command":"particle minecraft:smoke ~ ~1 ~ .2 .2 .2 .05 5 normal",
			"condition":{
				"type":"origins:power_active",
				"power":"*:*_toggle"
			}
		}
	},
	"wither":{
		"type":"origins:stacking_status_effect",
		"min_stacks":0,
		"max_stacks":1,
		"duration_per_stack":20,
		"effect":{
			"show_particles":false,
			"show_icon":false,
			"effect":"minecraft:wither",
			"amplifier":1
		},
		"condition":{
			"type":"origins:resource",
			"comparison":"==",
			"resource":"*:*_resource",
			"compare_to":0
		}
	},
	"phase":{
		"type":"origins:phasing",
		"blacklist":true,
		"render_type":"blindness",
		"view_distance":20,
		"condition":{
			"type":"origins:power_active",
			"power":"*:*_toggle"
		},
		"block_condition":{
			"type":"origins:in_tag",
			"tag":"origins:unphasable"
		},
		"phase_down_condition":{
			"type":"origins:and",
			"conditions":[
				{
					"type":"origins:power_active",
					"power":"*:*_toggle"
				},
				{
					"type":"origins:or",
					"conditions":[
						{
							"type":"origins:swimming"
						},
						{
							"type":"origins:sneaking"
						}
					]
				}
			]
		}
	},
	"flight":{
		"type":"origins:creative_flight",
		"condition":{
			"type":"origins:power_active",
			"power":"*:*_toggle"
		}
	},
	"respawn":{
		"type":"origins:action_on_callback",
		"entity_action_respawned":{
			"type":"origins:execute_command",
			"command":"resource set @s apolloparrot:shadow/cloud_resource 300"
		}
	}
}