{
	"type":"origins:multiple",
	"resource":{
		"type":"origins:resource",
		"max":300,
		"min":0,
		"start_value":300,
		"hud_render":{
			"sprite_location":"origins:textures/gui/community/huang/resource_bar_01.png",
			"bar_index":3
		}
	},
	"toggle":{
		"type":"origins:toggle",
		"key":"key.saveToolbarActivator",
		"active_by_default":false
	},
	"weak":{
		"type":"origins:stacking_status_effect",
		"condition":{
			"type":"origins:power_active",
			"power":"*:*_toggle"
		},
		"min_stacks":0,
		"max_stacks":1,
		"duration_per_stack":20,
		"effect":{
			"show_particles":false,
			"show_icon":false,
			"effect":"minecraft:weakness",
			"amplifier":100
		}
	},
	"timer":{
		"type":"origins:action_over_time",
		"interval":1,
		"entity_action":{
			"type":"origins:if_else",
			"condition":{
				"type":"origins:power_active",
				"power":"*:*_toggle"
			},
			"if_action":{
				"type":"origins:change_resource",
				"resource":"*:*_resource",
				"change":-1
			},
			"else_action":{
				"type":"origins:change_resource",
				"resource":"*:*_resource",
				"change":1
			}
		}
	},
	"wither":{
		"type":"origins:stacking_status_effect",
		"min_stacks":0,
		"max_stacks":1,
		"duration_per_stack":20,
		"effect":{
			"show_particles":false,
			"show_icon":false,
			"effect":"minecraft:wither",
			"amplifier":1
		},
		"condition":{
			"type":"origins:resource",
			"comparison":"==",
			"resource":"*:*_resource",
			"compare_to":0
		}
	},
	"float":{
		"type":"origins:stacking_status_effect",
		"condition":{
			"type":"origins:power_active",
			"power":"*:*_toggle"
		},
		"min_stacks":0,
		"max_stacks":1,
		"duration_per_stack":20,
		"effect":{
			"effect":"minecraft:levitation",
			"show_particles":false,
			"show_icon":false,
			"amplifier":1
		}
	},
	"respawn":{
		"type":"origins:action_on_callback",
		"entity_action_respawned":{
			"type":"origins:execute_command",
			"command":"resource set @s apolloparrot:wailing-one/float_resource 300"
		}
	}
}