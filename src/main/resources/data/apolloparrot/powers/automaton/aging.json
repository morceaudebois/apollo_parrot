{
	"type":"origins:multiple",
	"hidden":true,
	"exhaust":{
		"type":"origins:action_over_time",
		"interval":1,
		"entity_action":{
			"type":"origins:execute_command",
			"command":"function apolloparrot:automaton/exhaust"
		},
		"condition":{
			"type":"origins:or",
			"conditions":[
				{
					"type":"origins:sprinting"
				},
				{
					"type":"origins:swimming"
				},
				{
					"type":"origins:on_block",
					"inverted":true
				}
			]
		}
	},
	"decrease":{
		"type":"origins:action_over_time",
		"interval":10,
		"entity_action":{
			"type":"origins:execute_command",
			"command":"function apolloparrot:automaton/exhaust"
		}
	},
	"empty":{
		"type":"origins:action_over_time",
		"interval":400,
		"entity_action":{
			"type":"origins:execute_command",
			"command":"function apolloparrot:automaton/empty"
		},
		"condition":{
			"type":"origins:and",
			"conditions":[
				{
					"type":"origins:resource",
					"resource":"apolloparrot:automaton/firebar",
					"comparison":">",
					"compare_to":0
				},
				{
					"type":"origins:resource",
					"resource":"apolloparrot:automaton/waterbar",
					"comparison":">",
					"compare_to":0
				}
			]
		}
	}
}