		},
		"condition":{
			"type":"origins:resource",
			"resource":"*:*_resource",
			"comparison":">=",
			"compare_to":1
		}
	},
	"respawn":{
		"type":"origins:action_on_callback",
		"entity_action_respawned":{
			"type":"origins:execute_command",
			"command":"resource set @s apolloparrot:blob/health_bar_resource 1000"
		}
