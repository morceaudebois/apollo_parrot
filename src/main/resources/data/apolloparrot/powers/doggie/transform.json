{
  "type":"origins:active_self",
  "key":"key.origins.primary_active",

  "entity_action": {
    "type": "origins:if_else",
    "condition": {
      "type": "origins:command",
      "command": "execute if entity @s[nbt={CurrentIdentity:{id:\"minecraft:wolf\"}}]",
      "comparison": "==",
      "compare_to": 1
    },
    "if_action": {
      "type": "origins:execute_command",
      "command": "identity unequip @s"
    },
    "else_action": {
      "type": "origins:execute_command",
      "command": "identity equip @s minecraft:wolf {Tags:[\"charm_texture=wolf/nlg_wolf18.png\"]}"
    }
  }
}
