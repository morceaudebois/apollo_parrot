{
  "type": "origins:multiple",

  "transform": {
    "type":"origins:active_self",
    "key":"key.origins.primary_active",
    "entity_action": {
      "type": "origins:execute_command",
      "command": "identity equip @s minecraft:wolf"
    },
    "condition": {
      "type": "origins:command",
      "command": "execute if entity @s[nbt={CurrentIdentity:{id:\"minecraft:wolf\"}}]",
      "comparison": "!=",
      "compare_to": 1
    }
  },

  "transform_back": {
    "type":"origins:active_self",
    "key":"key.origins.primary_active",
    "entity_action": {
      "type": "origins:execute_command",
      "command": "identity unequip @s"
    },
    "condition": {
      "type": "origins:command",
      "command": "execute if entity @s[nbt={CurrentIdentity:{id:\"minecraft:wolf\"}}]",
      "comparison": "==",
      "compare_to": 1
    }
  }
}
